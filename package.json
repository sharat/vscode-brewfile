{
    "name": "vscode-brewfile",
    "displayName": "Brewfile",
    "description": "Syntax Highlighting for Brewfile",
    "version": "1.0.0",
    "publisher": "sarath-c",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:Brewfile"
    ],
    "main": "./extension",
    "contributes": {
        "languages": [{
			"id": "ruby",
			"aliases": ["Ruby", "ruby"],
            "filenamePatterns": [
                "Brewfile"
            ]
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}